{
  "data_dir": "/path/to/data_dir",
  "model_dir": "same",
  "overwrite": false,
  "random_state": 42,

  "data_prefix": ["T1_w"],
  "label_prefix": ["breast_mask"],
  "mask_prefix": ["breast_mask"],
  "mask_dilate": [160, 160, 160],
  "filter_zero": 0,

  "dimension_mode": "3D",
  "data_plane": "ax",
  "train_dims": [80, 80, 80],
  "train_patch_overlap": [3, 3, 3],
  "infer_dims": [200, 200, 200],
  "infer_patch_overlap": [1.5, 1.5, 1.5],
  "augment_train_data": true,
  "label_interp": 0,
  "mask_weights": false,
  "metrics": ["binary_crossentropy"],

  "norm_data": true,
  "norm_labels": false,
  "norm_mode": "zscore",

  "model_name": "unet_3d_bneck",
  "base_filters": 32,
  "output_filters": 1,
  "layer_layout": [3, 4, 5],
  "final_layer": "sigmoid",
  "kernel_size": [3, 3, 3],
  "data_format": "channels_last",
  "activation": "leaky_relu",
  "optimizer": "adam",
  "dist_strat": "none",
  "mixed_precision": false,

  "shuffle_size": 2000,
  "batch_size": 4,
  "samples_per_epoch": 16000,
  "num_threads": 4,
  "train_fract": 1.0,
  "learning_rate": [0.001, 12, 0.5],
  "learning_rate_decay": "simple_step",
  "loss": "combo_loss3d",
  "num_epochs": 20,
  "dropout_rate": 0.3
}
