{
  "data_dir": "/path/to/data_dir",
  "model_dir": "same",
  "overwrite": true,
  "random_state": 42,

  "data_prefix": ["ADC_wm", "B0_wm", "DWI_wm", "T1_wm", "T1FS_wm", "T2FS_wm"],
  "label_prefix": ["T1gad_wm"],
  "mask_prefix": ["breast_mask"],
  "mask_dilate": [0, 0, 0],
  "filter_zero": 0.2,

  "dimension_mode": "3D",
  "data_plane": "ax",
  "train_dims": [80, 80, 80],
  "train_patch_overlap": [3, 3, 3],
  "infer_dims": [280, 280, 280],
  "infer_patch_overlap": [2, 2, 2],
  "augment_train_data": true,
  "label_interp": 1,
  "mask_weights": false,
  "metrics": ["mae"],
  
  "norm_data": true,
  "norm_labels": true,
  "norm_mode": "zscore",

  "model_name": "unet_3d_bneck2",
  "base_filters": 32,
  "output_filters": 1,
  "layer_layout": [3, 4, 5],
  "final_layer": "conv",
  "kernel_size": [3, 3, 3],
  "data_format": "channels_last",
  "activation": "leaky_relu",
  "mixed_precision": false,
  "dist_strat": "none",

  "shuffle_size": 500,
  "batch_size": 5,
  "num_threads": 1,
  "samples_per_epoch": 40000,
  "train_fract": 0.85,
  "learning_rate": [0.001, 15, 0.5],
  "learning_rate_decay": "simple_step",
  "optimizer": "adam",
  "loss": "MSE",
  "num_epochs": 20,
  "dropout_rate": 0.3
}
